{
  "name": "@vibecode/blueprint",
  "version": "1.0.0",
  "description": "Collaborative Vibecoding Blueprint - A template for AI-assisted collaborative development projects",
  "type": "module",
  "main": "src/setup.js",
  "bin": {
    "vibecode-setup": "./src/setup.js"
  },
  "scripts": {
    "setup": "node src/setup.js",
    "test-setup": "node src/test-setup.js",
    "dev": "node src/setup.js --dev",
    "test": "node --test test/**/*.test.js",
    "lint": "eslint src/ templates/",
    "format": "prettier --write .",
    "validate": "node src/validate.js"
  },
  "keywords": [
    "ai-coding",
    "collaborative-development",
    "blueprint",
    "template",
    "vibecoding",
    "monorepo"
  ],
  "author": "Vibecode Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "pnpm": {
    "overrides": {
      "semver": "^7.5.4"
    }
  }
}
